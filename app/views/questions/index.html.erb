<h2>Questions</h2>

<div class="row">
  <div class="col-md-12">
    <table class="table-responsive striped">
      <thead class="thad-light">
        <tr>
          <th>ID</th>
          <th>Title</th>
          <th>Menu</th>
        </tr>
      </thead>
      <tbody>
      <% @questions.each do |question| %>
        <tr>
          <td><%= question.id %></td>
          <td><%= question.title %></td>
          <td>[Edit] [Delete]</td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
